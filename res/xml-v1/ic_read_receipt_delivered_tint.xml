<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <bitmap android:src="@drawable/ic_read_receipt_delivered" android:tint="@color/arcane_black" />
    </item>
    <item android:state_focused="true">
        <bitmap android:src="@drawable/ic_read_receipt_delivered" android:tint="@color/arcane_black" />
    </item>
    <item android:state_pressed="true">
        <bitmap android:src="@drawable/ic_read_receipt_delivered" android:tint="@color/arcane_black" />
    </item>
    <item>
        <bitmap android:src="@drawable/ic_read_receipt_delivered" android:tint="@color/theme" />
    </item>
</selector>