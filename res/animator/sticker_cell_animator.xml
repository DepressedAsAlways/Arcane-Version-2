<?xml version="1.0" encoding="utf-8"?>
<set android:ordering="sequentially"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <set android:ordering="together">
        <objectAnimator android:duration="75" android:valueFrom="1.0" android:valueTo="1.05" android:propertyName="scaleX" />
        <objectAnimator android:duration="75" android:valueFrom="1.0" android:valueTo="1.05" android:propertyName="scaleY" />
    </set>
    <set android:ordering="together">
        <objectAnimator android:interpolator="@android:anim/overshoot_interpolator" android:duration="150" android:valueFrom="1.05" android:valueTo="1.0" android:propertyName="scaleX" />
        <objectAnimator android:interpolator="@android:anim/overshoot_interpolator" android:duration="150" android:valueFrom="1.05" android:valueTo="1.0" android:propertyName="scaleY" />
    </set>
</set>